            "templateData": "<table class=\"table\">\r\n  <thead>\r\n  <tr>\r\n    <th scope=\"col\" class=\"table-head\">\r\n      <div class=\"col-head\">\r\n        {{filterOrders[0].columnLabel}}\r\n        <i class=\"bi bi-arrow-up\" (click)=\"updateFilterOrder($event, filterOrders[0])\"></i>\r\n      </div>\r\n    </th>\r\n    <th scope=\"col\" class=\"table-head\">\r\n     <div class=\"col-head\">\r\n       {{filterOrders[1].columnLabel}}\r\n       <i class=\"bi bi-arrow-up\" (click)=\"updateFilterOrder($event, filterOrders[1])\"></i>\r\n     </div>\r\n    </th>\r\n    <th scope=\"col\" class=\"table-head\">\r\n     <div class=\"col-head\">\r\n       {{filterOrders[2].columnLabel}}\r\n       <i class=\"bi bi-arrow-up\" (click)=\"updateFilterOrder($event,filterOrders[2])\"></i>\r\n     </div>\r\n    </th>\r\n    <th scope=\"col\" class=\"table-head\">\r\n      <div class=\"col-head\">\r\n        {{filterOrders[3].columnLabel}}\r\n        <i class=\"bi bi-arrow-up\" (click)=\"updateFilterOrder($event, filterOrders[3])\"></i>\r\n      </div>\r\n    </th>\r\n    <th scope=\"col\" class=\"table-head\">\r\n      <div class=\"col-head\">\r\n        {{filterOrders[4].columnLabel}}\r\n        <i class=\"bi bi-arrow-up\" (click)=\"updateFilterOrder($event, filterOrders[4])\"></i>\r\n      </div>\r\n    </th>\r\n    <th scope=\"col\" class=\"table-head\">\r\n      <div class=\"col-head\">\r\n        {{filterOrders[5].columnLabel}}\r\n        <i class=\"bi bi-arrow-up\" (click)=\"updateFilterOrder($event, filterOrders[5])\"></i>\r\n      </div>\r\n    </th>\r\n    <th scope=\"col\" class=\"table-head\">\r\n      <div class=\"col-head\">\r\n        {{filterOrders[6].columnLabel}}\r\n        <i class=\"bi bi-arrow-up\" (click)=\"updateFilterOrder($event,filterOrders[6])\"></i>\r\n      </div>\r\n    </th>\r\n  </tr>\r\n  </thead>\r\n  <tbody>\r\n    @for (vacant of vacants; track vacant.id) {\r\n      <tr>\r\n        <td>{{vacant.ticketNumber}}</td>\r\n        <td>{{vacant.vacantName}} </td>\r\n        <td>\r\n          <a href=\"#\">\r\n            <i class=\"bi bi-book\"></i>\r\n          </a>\r\n        </td>\r\n        <td>\r\n          <app-dropdown></app-dropdown>\r\n        </td>\r\n        <td>\r\n          <span class=\"badge badge-pill bg-primary\">{{vacant.status}}</span>\r\n        </td>\r\n        <td>\r\n          <app-dropdown></app-dropdown>\r\n        </td>\r\n        <td>{{vacant.comments}}</td>\r\n      </tr>\r\n    } @empty {\r\n      <tr>\r\n        <td colspan=\"7\">No hay vacantes</td>\r\n      </tr>\r\n    }\r\n  </tbody>\r\n</table>\r\n"
